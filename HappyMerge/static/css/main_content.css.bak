.background_image{
   position: fixed;
   left: 0;
   right: 0;
   z-index: 1;
   width: 1200px;
   height: 800px;
   background-image: url('../static/images/bg_logo.png');
  -webkit-filter: blur(5px);
  -moz-filter: blur(5px);
  -o-filter: blur(5px);
  -ms-filter: blur(5px);
  filter: blur(5px);
}
.left_block{
    display: block;
    float: left;
    max-width: 60%;
}
.right_block{
    display: block;
    float: right;
    max-width: 40%;
}

#border_stuff{
    padding: 2px;
    border: 3px solid none;
    border-radius:0.3em;
    background-color: #CCCCCC;
}

ul{
    list-style-type: none;
}
